CREATE TABLE IF NOT EXISTS public.child_activitiescallemail_ticket_detail
(
    id bigint,
    messageid bigint,
    tagid bigint,
    subject character varying COLLATE pg_catalog."default",
    assigned character varying COLLATE pg_catalog."default",
    created_on timestamp without time zone,
    activityname character varying COLLATE pg_catalog."default",
    "Assigned Role" text COLLATE pg_catalog."default",
    time_spent bigint,
    time_log_date timestamp with time zone,
    "Call in Time" character varying COLLATE pg_catalog."default",
    CONSTRAINT activitycallemail_key UNIQUE (messageid, tagid)
)

CREATE TABLE IF NOT EXISTS public.field_options_values
(
    id bigint NOT NULL,
    field_name text COLLATE pg_catalog."default",
    option_value text COLLATE pg_catalog."default",
    last_modified_on timestamp with time zone,
    CONSTRAINT field_option_db3_pkey PRIMARY KEY (id)
)

CREATE TABLE IF NOT EXISTS public.incremental_refresh_detail
(
    id integer NOT NULL DEFAULT nextval('incremental_refresh_detail_id_seq'::regclass),
    recordcount character varying COLLATE pg_catalog."default",
    modified_on timestamp without time zone,
    CONSTRAINT incremental_refresh_detail_pkey PRIMARY KEY (id)
)
CREATE TABLE IF NOT EXISTS public.deleted_iactivetickets
(
    id integer NOT NULL DEFAULT nextval('deleted_iactivetickets_id_seq'::regclass),
    recordcount character varying COLLATE pg_catalog."default",
    modified_on timestamp without time zone,
    CONSTRAINT deleted_refresh_pkey PRIMARY KEY (id)
)
